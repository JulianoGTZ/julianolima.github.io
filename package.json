    "katex": "^0.13.18",
    "lodash": "^4.17.15",
    "lost": "8.3.1",
    "moment": "^2.24.0",
    "node-notifier": "^10.0.0",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "postcss-pxtorem": "4.0.1",
    "prismjs": "^1.24.0",
    "prop-types": "^15.7.2",
    "react": "16.14.0",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
